{
  "name": "semi-design-admin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "preview": "vite preview",
    "build:dev": "tsc && vite build --mode dev",
    "build:test": "tsc && vite build --mode test",
    "build:prod": "tsc && vite build --mode prod",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "lint": "tsc --noEmit &&  eslint src --ext .ts,.tsx",
    "lint:fix": "tsc --noEmit && eslint --fix --ext src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.scss\" \"mock/**/*.ts\""
  },
  "dependencies": {
    "@douyinfe/semi-ui": "^2.76.1",
    "axios": "^1.8.3",
    "driver.js": "^1.3.5",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.25.0",
    "react-router-dom": "7.3.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "mockjs": "^1.1.0",
    "prettier": "^3.5.3",
    "qs": "^6.14.0",
    "sass": "^1.86.0",
    "scss": "^0.2.4",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-mock": "^3.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.scss": [
      "prettier --write"
    ]
  }
}
